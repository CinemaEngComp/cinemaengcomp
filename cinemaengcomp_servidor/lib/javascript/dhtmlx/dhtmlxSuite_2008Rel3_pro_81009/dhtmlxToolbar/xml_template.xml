<?xml version="1.0"?>
<toolbar>											top xml tag, mandatory, each row must have unique ID
	<item id="new" type="button" img="new.gif" imgdis="new_dis.gif"/>			button item (enabled/disabled images only)
	<item id="save" type="button" text="Save" img="save.gif" imgdis="save_dis.gif"/>	button item (text, images)
	<item id="open" type="button" text="Open"/>						button item (text only)
	
	<item id="sep01" type="separator"/>							separator item
	
	<item id="undo" type="button" img="undo.gif" imgdis="undo_dis.gif" title="Undo"/>	button item with tooltip
	<item id="redo" type="button" img="redo.gif" imgdis="redo_dis.gif" title="Redo"/>
	<item id="cut" type="button" img="cut.gif" imgdis="cut_dis.gif" title="Cut" enabled="false"/>	disabled button
	<item id="copy" type="button" img="copy.gif" imgdis="copy_dis.gif" title="Copy"/>
	<item id="paste" type="button" img="paste.gif" imgdis="paste_dis.gif" title="Paste"/>
	
	<item id="sep02" type="separator"/>
	
	
	<item id="form" type="buttonTwoState" img="form.gif" imgdis="form_dis.gif" title="Form"/>				two-state button (images, tooltip)
	<item id="filter" type="buttonTwoState" img="filter.gif" imgdis="filter_dis.gif" text="Filter" title="Filter"/>		two-state button (images, text, tooltip)
	<item id="any_word" type="buttonTwoState" text="Any Word" selected="true"/>						pressed two-state button (text only)
	
	<item id="txt_1" type="buttonInput"/>								input button
	<item id="txt_2" type="buttonInput" value="Filter Text" width="60" title="input text here"/>	input button (with predifined initial value, width and tooltip)
	
	<item id="sep1" type="separator"/>
	
	<item id="history" type="buttonSelect" text="History">						select button (text only)
		<item type="button" id="8_1" text="Today"/>						listed option (with text only)
		<item type="button" id="8_2" text="Yesturday" selected="true"/>				preselected listed option
		<item type="button" id="8_3" text="Last Week"/>
		<item type="button" id="8_4" text="Last Month"/>
		<item id="8_sep0" type="separator"/>							listed option - separator
		<item type="button" id="8_5" img="icon_more.gif" text="More..."/>			listed option (image, text)
	</item>
	
	<item id="sep2" type="separator"/>
	
	<item id="page_setup" type="buttonSelect" img="page_setup.gif" imgdis="page_setup_dis.gif" text="Page">		select button (text, images)
		<item type="button" id="9_11" text="Layout"/>
		<item type="button" id="9_12" text="Management"/>
	</item>
	
	<item id="print" type="buttonSelect" img="print.gif" imgdis="print_dis.gif" title="Print">			select button (images only, tooltip)
		<item type="button" id="9_1" text="Page"/>
		<item type="button" id="9_2" text="Selection"/>
		<item id="9_sep0" type="separator"/>
		<item type="button" id="9_3" text="Custom..."/>
	</item>
	
	<item id="sep3" type="separator"/>
	
	<item id="slider" type="slider" length="70" valueMin="10" valueMax="100" valueNow="70" textMin="10 MBit" textMax="100 MBit" toolTip="%v MBit"/>
	
			slider item
			mandatory: length - length in px, minValue (int), maxValue (int), valueNow - current value (set by the default, int)
			optional: textMin - label for minValue, textMax - lavel for maxValue, toolTip - tolltip template (%v will replaced with cureent value)
	
	<item id="sep4" type="separator"/>
	
	<item id="text" type="text" text="dhtmlxToolbar Demo"/>		text item

</toolbar>